<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundedCornersImageView">
        <attr name="rcl_cornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="image">
        <attr name="imageCorner" format="dimension" />
        <attr name="url" format="string" />
        <attr name="rounded" format="boolean" />
        <attr name="error" format="integer" />
        <attr name="drawable" format="integer" />
    </declare-styleable>


    <declare-styleable name="SwipeRefreshLayout">
        <attr name="presenter" format="reference"/>
        <attr name="episodesViewModel" format="reference"/>
        <attr name="image" format="reference"/>
        <attr name="onMenuItemClick" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SerialSearch">
        <attr name="addTextChangedListener" format="reference"/>
    </declare-styleable>
</resources>